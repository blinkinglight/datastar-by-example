package main

import "github.com/blinkinglight/datastar-by-example/shared"

templ Form() {
	@shared.Main() {
		<div>
            <form data-on-submit__prevent="@post('/', { contentType: 'form' })">
                <label for="name">Name:</label>
                <input type="text" id="name" name="name">
                <input type="submit" value="Submit">
            </form>
		</div>
	}
}